{
  server: {
    intercepts: {
      'github.githubassets.com': {
        '.*': {
          proxy: 'assets.fastgit.org',
          backup: ['github.githubassets.com'],
          sni: null,
        }
      }
    },
    dns: {
      mapping: {
        '*jetbrains.com': 'quad9',
      },
      speedTest: {
        interval: 60000,
      },
    }
  }
}
